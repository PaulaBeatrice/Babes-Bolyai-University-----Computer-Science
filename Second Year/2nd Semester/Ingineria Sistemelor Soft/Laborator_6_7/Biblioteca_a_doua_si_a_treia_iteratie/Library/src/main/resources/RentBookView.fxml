<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="499.0" prefWidth="884.0" style="-fx-background-color: peachpuff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RentBookController">
   <children>
      <TableView fx:id="tabelBooks" layoutX="70.0" layoutY="36.0" onMouseClicked="#pickBook" prefHeight="314.0" prefWidth="727.0">
         <columns>
            <TableColumn fx:id="clmId" prefWidth="51.0" text="id" />
            <TableColumn fx:id="clmTitle" prefWidth="194.0" text="Title" />
            <TableColumn fx:id="clmAutor" prefWidth="211.0" text="Author" />
            <TableColumn fx:id="clmEditura" prefWidth="193.0" text="Publishing house" />
            <TableColumn fx:id="clmAn" prefWidth="77.0" text="Year" />
         </columns>
      </TableView>
      <Button layoutX="70.0" layoutY="397.0" mnemonicParsing="false" onMouseClicked="#showRentedBooksButton" prefHeight="35.0" prefWidth="240.0" style="-fx-background-color: mistyrose; -fx-font-family: Georgia; -fx-font-size: 18px; -fx-border-color: black;" text="Show selected book" />
      <Button layoutX="504.0" layoutY="397.0" mnemonicParsing="false" onMouseClicked="#finishRentalClick" prefHeight="35.0" prefWidth="290.0" style="-fx-background-color: mistyrose; -fx-font-family: Georgia; -fx-font-size: 18px; -fx-border-color: black;" text="Finish Rental" />
   </children>
</AnchorPane>
